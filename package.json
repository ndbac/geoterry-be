{
  "name": "geoterry-backend",
  "version": "0.0.1",
  "description": "backend service for geoterry app",
  "author": "metaverest",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "format:check": "prettier --check \"{src,test}/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,test}/**/*.ts\"",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch --runInBand --detectOpenHandles --forceExit",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test:github:ci": "cross-env NODE_ENV=test jest --silent --ci --coverage --testLocationInResults --json --outputFile=report.json --runInBand --detectOpenHandles --forceExit",
    "prepare": "husky install",
    "migrate:create": "sh -c 'ts-node node_modules/migrate-mongo/bin/migrate-mongo.js create $0 -f migrate-mongo-config.ts' ",
    "migrate:down": "ts-node --transpile-only -r tsconfig-paths/register node_modules/.bin/migrate-mongo down -f ./migrate-mongo-config.ts",
    "migrate:up": "ts-node --transpile-only -r tsconfig-paths/register node_modules/.bin/migrate-mongo up -f ./migrate-mongo-config.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.3.2",
    "@nestjs/swagger": "^6.1.4",
    "@sentry/minimal": "^6.19.7",
    "@sentry/node": "^7.34.0",
    "aws-sdk": "^2.1304.0",
    "axios": "^1.2.5",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express-basic-auth": "^1.2.1",
    "firebase-admin": "^11.10.1",
    "get-stream": "6.0.1",
    "haversine-distance": "^1.2.1",
    "heic-convert": "^1.2.4",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "lint-staged": "^13.1.0",
    "minimist": "^1.2.7",
    "mongoose": "^6.9.0",
    "nestjs-i18n": "^8.1.2",
    "nestjs-pino": "^3.1.2",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "slugify": "^1.6.5",
    "swagger-ui-express": "^4.6.0",
    "twilio": "^3.84.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.13",
    "@types/heic-convert": "^1.2.0",
    "@types/jest": "28.1.8",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/qrcode": "^1.5.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commitlint": "^17.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "28.1.3",
    "migrate-mongo": "^9.0.0",
    "mongodb-memory-server": "^8.11.4",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts",
      "!<rootDir>/src/db-migrations/**/*.ts"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/test.setup.ts"
    ],
    "testTimeout": 20000
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --cache --fix --max-warnings=0",
      "prettier --write"
    ],
    "test/**/*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
